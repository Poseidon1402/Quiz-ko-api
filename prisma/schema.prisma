generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://quiz-ko:quiz-ko_password@localhost:5432/quiz-ko?schema=public"
}

model Example {
  id      Int     @default(autoincrement())   @id
  firstName     String
}